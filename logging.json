{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "default": {
      "format": "%(asctime)s %(levelname)s %(name)s %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "INFO",
      "formatter": "default",
      "stream": "ext://sys.stdout"
    },
    "file": {
      "class": "logging.handlers.RotatingFileHandler",
      "level": "INFO",
      "formatter": "default",
      "filename": "app.log",
      "maxBytes": 10485760,
      "backupCount": 3,
      "encoding": "utf8"
    }
  },
  "root": {
    "level": "WARNING",
    "handlers": ["console", "file"]
  },
  "loggers": {
    "reasoning_benchmark": {
      "level": "INFO",
      "handlers": ["console", "file"],
      "propagate": false
    }
  }
}
